<NotepadPlus>
	<functionList>
		<!-- ================================ [ Batch / Command Shell Script ] -->

		<parser
			displayName="Arcsys BBS"
			id         ="bbs_label"
			commentExpr="(?s:\x2F\x2A.*?\x2A\x2F)"
